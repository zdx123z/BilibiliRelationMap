{
  "name": "react-userscript",
  "version": "0.2.0",
  "description": "React + TypeScript + Ant Design 油猴脚本",
  "type": "module",
  "main": "dist/script.user.js",
  "scripts": {
    "build": "webpack --config webpack.config.cjs",
    "build:patch": "node scripts/bump-version.cjs patch && webpack --config webpack.config.cjs",
    "build:minor": "node scripts/bump-version.cjs minor && webpack --config webpack.config.cjs",
    "dev": "webpack --watch --config webpack.config.cjs",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "userscript",
    "react",
    "typescript",
    "antd"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/tampermonkey": "^5.0.3",
    "@types/webpack": "^5.28.5",
    "css-loader": "^6.8.1",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@cosmograph/cosmograph": "^1.4.2",
    "antd": "^5.12.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
